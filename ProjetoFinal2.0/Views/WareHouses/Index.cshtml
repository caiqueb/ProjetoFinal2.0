@model IEnumerable<ProjetoFinal2._0.Models.WareHouse>

@{
    ViewBag.Title = "Index";
}

<h2>Armazéns</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cities.Name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Departments.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cities.Name)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Departments.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.WareHouseId }) |
                @Html.ActionLink("Details", "Details", new { id = item.WareHouseId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.WareHouseId })
            </td>
        </tr>
    }

</table>
